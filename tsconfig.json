{
  "compilerOptions": {
    "target": "es2016",
    "module": "CommonJS",
    "rootDir": "src",
    "outDir": "./dist",
    "esModuleInterop": true,
    "moduleResolution": "Node",
    "baseUrl": "src",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "paths": {
      "@/*": ["*"]
    }
  }
}
